{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["POST", "GET", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  },
  "env": {
    "MY_ENV_VARIABLE": "value"
  },
  "routes": [
    {
      "src": "/old-route",
      "status": 301,
      "headers": {
        "Location": "/new-route"
      }
    }
  ]
}
